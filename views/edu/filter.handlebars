 {{> navbar this}}
 <button id="filterButton">Filter</button>
  <ul id="courseList"></ul>
<section>
<a href="/viewAllCourses" class="anotherSearch">Make another search</a>
<h1 class="resultH1">Following are the results for "{{filterSearchTerm}}"</h1>
 <ul>
    {{#each data}}
    <li class="courseList"><a class="courseAnchor" href="/course/{{courseName}}">{{courseName}} </a></li>
    {{/each}}
</ul>

{{#if hasError}}
<p class="notFound">{{error}}</p>
{{/if}}
</section>